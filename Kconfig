menu "Zephyr Kernel"
    source "Kconfig.zephyr"
endmenu

menu "ZSPSUMon"

    config PICO_DISPLAY_PACK
        bool
    prompt "Pico Display Pack"
    default n

    config PICO_DISPLAY_PACK2
        bool
    prompt "Pico Display Pack2"
    default n

    config PICO2_DISPLAY_PACK
        bool
    prompt "Pico2 Display Pack"
    default n

    config PICO2_DISPLAY_PACK2
        bool
    prompt "Pico2 Display Pack2"
    default n

endmenu
